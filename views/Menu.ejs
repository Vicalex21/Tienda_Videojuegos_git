<script>
document.addEventListener('DOMContentLoaded', function () {
  const carouselElem = document.querySelector('.carousel');
  const logos = document.querySelectorAll('.logo-slide');
  const carousel = M.Carousel.init(carouselElem, {
    fullWidth: true,
    indicators: true,
    duration: 200
  });

  function syncLogoWithIndicator() {
    const indicators = document.querySelectorAll('.carousel .indicators .indicator-item');
    let idx = 0;
    indicators.forEach((el, i) => {
      if (el.classList.contains('active')) idx = i;
    });
    logos.forEach((logo, i) => {
      logo.classList.toggle('active', i === idx);
    });
  }

  // Sincroniza el logo al cargar la página
  syncLogoWithIndicator();

  // Autoplay: avanza cada 4 segundos, pero el logo se sincroniza SOLO cuando el slide cambia
  setInterval(function () {
    carousel.next();
    // El logo se sincroniza cuando el slide realmente cambia (evento)
  }, 4000);

  // Sincroniza el logo SIEMPRE que cambie el slide (manual o autoplay)
  carouselElem.addEventListener('carouselNext', syncLogoWithIndicator);
  carouselElem.addEventListener('carouselPrev', syncLogoWithIndicator);
  carouselElem.addEventListener('carouselSet', syncLogoWithIndicator);

  // También sincroniza al hacer clic en los indicadores
  document.querySelector('.carousel .indicators').addEventListener('click', function() {
    setTimeout(syncLogoWithIndicator, 220);
  });

  // Por seguridad, sincroniza el logo después de cada animación de transición
  carouselElem.addEventListener('transitionend', syncLogoWithIndicator);
});
</script>



