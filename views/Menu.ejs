.parallax-container {
  position: relative;
  overflow: hidden;
  height: 60vh; 
  margin-bottom: 0;
  border-radius: 20px;
  box-shadow: 0 8px 32px 0 rgba(31, 38, 135, 0.37);
}

.parallax-inner {
  position: absolute;
  top: 0; left: 0;
  width: 100%; height: 100%;
  overflow: hidden;
}

.parallax-inner img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
  filter: brightness(0.85) saturate(1.2) contrast(1.1);
  transition: filter 0.4s, transform 0.4s;
  border-radius: 20px;
}

.parallax-container:hover .parallax-inner img {
  filter: brightness(1) saturate(1.4) contrast(1.2);
  transform: scale(1.05);
  box-shadow: 0 0 40px #7c4dff77;
}
.parallax-overlay {
  position: absolute;
  top: 0; left: 0;
  width: 100%; height: 100%;
  background: linear-gradient(180deg, rgba(40,0,80,0.25) 0%, rgba(0,0,0,0.45) 100%);
  pointer-events: none;
  border-radius: 20px;
}







